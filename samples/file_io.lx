# simple code demonstrating variable-parameter functions
#   as well as reading/writing files

print("hello", "world", "goodbye all");

f = open("data/test1.txt", "r");
contents1 = read(f);
contents2 = read(f);

print(contents1);
print(contents2);
print(type(f));
print(type(read(f)));

close(f);

f = open("data/test2.txt", "a");
write(f, contents1);
close(f);
