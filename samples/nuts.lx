print("started calling functions");

def say_hello(to, whom) {
    print(to, whom);
}

def this_can_see_globals() {
    return pi;
}

def but_locals_take_precedence(something) {
    e = something;
    return e;
}

say_hello("Hello", "World");
print(this_can_see_globals());
say_hello("Goodbye", "Joe");

print(e, but_locals_take_precedence(3.14), e);

print("finished calling functions");
