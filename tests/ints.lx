# Integers
assert(1 + 1 == 2);
assert(42 + 42 == 84);
assert(-56 + 21 == -35);
assert(-687 + -983 == -1670);

assert(3 - 2 == 1);
assert(147 - 68 == 79);
assert(2 - 1111 == -1109);
assert(-67 - 78 == -145);
assert(-123 - -98380 == 98257);

assert(2 * 7 == 14);
assert(123 * 31 == 3813);
assert(-65 * 23 == -1495);
assert(-32 * -31 == 992);

assert(520 / 5 == 104);
assert(7 / 3 == 2);
assert(67 / -6 == -11);
assert(-78 / 4 == -19);
assert(-88 / -11 == 8);

assert(2 ** 2 == 4);
assert(42 ** 4 == 3111696);
assert(1 ** -1 == 1);
assert(-5 ** 3 == -125);

assert(45 % 4 == 1);
assert(21 % -2 == 1);
assert(-32 % 7 == -4);
assert(-99 % -37 == -25);
